#include <iostream>
#include <vector>

using namespace std;

vector<string> punch(vector<string> &v, vector<string>::iterator
it,int k) {
    if (it - k < v.begin()) {
        v.erase(v.begin(), min(v.end(), it + k + 1));
    }
    else if (it + k + 1 > v.end()) {
        v.erase(max(v.begin(), it - k), v.end());
    }
    else
        v.erase(max(v.begin(), it - k), min(v.end(), it + k + 1));
    return v;
    }

int main() {
    int n,j,k;
    cin >> n >> j >> k;
    vector<string> v(n);
    for (int i = 0;i < n;i++) {
        cin >> v[i];
    }

    cout << "Result after punch" << endl;
    vector<string> result = punch(v, v.begin() + j, k);
    for (auto &x : result) {
        cout << x << endl;
    }
}
